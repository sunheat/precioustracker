<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maxsoft.precioustracker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light" >
        <activity
            android:name="com.maxsoft.precioustracker.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.maxsoft.precioustracker.DisplayMoveActivity"
            android:label="@string/title_activity_display_move"
            android:parentActivityName="com.maxsoft.precioustracker.MainActivity" >
        </activity>
        <activity
            android:name="com.maxsoft.precioustracker.AddMoveActivity"
            android:label="@string/title_activity_add_move"
            android:parentActivityName="com.maxsoft.precioustracker.MainActivity" >
        </activity>
        <activity
            android:name="com.maxsoft.precioustracker.CreateItemActivity"
            android:label="@string/title_activity_create_item" >
        </activity>
    </application>

</manifest>